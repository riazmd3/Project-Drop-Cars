{
  "expo": {
    "name": "Drop cars2",
    "slug": "dropcars3",
    "version": "1.0.0",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "31f4ffee-37b4-4db6-a102-1cd99bd74f8e"
      }
    },
    "android": {
      "package": "com.dropcars.driverapp",
      "googleServicesFile": "./assets/google-services.json"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/31f4ffee-37b4-4db6-a102-1cd99bd74f8e"
    },
    "owner": "dropcars",
    "ios": {}
  }
}
