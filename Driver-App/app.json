{
  "expo": {
    "name": "Drop cars2",
    "slug": "drop-cars2",
    "version": "1.0.0",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
        [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ce3d1356-cccc-4b4c-bb24-a1aa39876e8e"
      }
    },
    "android": {
      "package": "com.dropcars.driverapp",
      "googleServicesFile": ".android/app/google-services.json"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ce3d1356-cccc-4b4c-bb24-a1aa39876e8e"
    },

    "owner": "dropcars"


  }
}
