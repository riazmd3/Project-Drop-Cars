{
  "expo": {
    "name": "Drop Cars Vendor",
    "slug": "drop-cars-vendor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ad24ec4-662c-4da2-bc6e-17b09235aab1"
      }
    },
    "android": {
      "package": "com.pugazheas.dropcarsvendor"
    }
  }
}
